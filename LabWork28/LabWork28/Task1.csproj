<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Roboto-VariableFont_wdth,wght.ttf" />
    <None Remove="galaxy.jpg" />
    <None Remove="Images\galaxy.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="bin\Debug\net9.0-windows\" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\Roboto-VariableFont_wdth,wght.ttf" />
    <Resource Include="galaxy.jpg" />
    <Resource Include="Images\galaxy.jpg" />
  </ItemGroup>

</Project>
