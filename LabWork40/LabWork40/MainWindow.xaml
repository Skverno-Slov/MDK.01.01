<Window x:Class="LabWork40.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork40"
        mc:Ignorable="d"
        Title="Графика" Height="700" Width="300">
    <StackPanel Orientation="Horizontal">
        <StackPanel>
            <Ellipse Width="100" Height="100" Stroke="Black" Fill="Blue"/>
            <Ellipse Width="100" Height="100" Stroke="Black">
                <Ellipse.Fill>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="Red" Offset="0"/>
                        <GradientStop Color="Yellow" Offset="1"/>
                    </LinearGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse Width="100" Height="100" Stroke="Black">
                <Ellipse.Fill>
                    <RadialGradientBrush>
                        <GradientStop Color="Green" Offset="0"/>
                        <GradientStop Color="White" Offset="1"/>
                    </RadialGradientBrush>
                </Ellipse.Fill>
            </Ellipse>
            <Ellipse Width="100" Height="100" Stroke="Black">
                <Ellipse.Fill>
                    <ImageBrush ImageSource="/image.png"/>
                </Ellipse.Fill>
            </Ellipse>
        </StackPanel>

        <StackPanel>
            <Rectangle Width="100" Height="100" Stroke="Black" Fill="Purple"/>
            <Rectangle Width="100" Height="100" Stroke="Black">
                <Rectangle.Fill>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="Blue" Offset="0"/>
                        <GradientStop Color="Cyan" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
            <Rectangle Width="100" Height="100" Stroke="Black">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="/image.png"/>
                </Rectangle.Fill>
            </Rectangle>
            <Rectangle Width="100" Height="100" Stroke="Black" RadiusX="10" RadiusY="10" Fill="Orange"/>
            <Rectangle Width="100" Height="100" Stroke="Green" StrokeDashArray="2,2" Fill="LightBlue"/>
        </StackPanel>
    </StackPanel>
</Window>
