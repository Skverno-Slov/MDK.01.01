<Window x:Class="LabWork46.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork46"
        mc:Ignorable="d"
        Title="Главное окно" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="Введите запрос"/>
        <TextBox x:Name="QueryTextBox"/>
        <Button x:Name="ExecuteButton" Content="Выполнить запрос" Click="ExecuteButton_Click"/>
        <TextBlock x:Name="ResultTextBlock"/>
    </StackPanel>
</Window>
